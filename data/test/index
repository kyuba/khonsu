(available-languages "en")

(object
  (page
    "test page"
    (formal-paragraph "title (common)" "common text 1" "common text 3")
    (if (equal? language "en")
        (formal-paragraph "title (EN)" "lorem ipsum... (EN)")
        (formal-paragraph "title" "lorem ipsum..."))
    (formal-paragraph "title (common)" "common text 2")))
